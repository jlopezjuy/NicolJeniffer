<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="e.dominio.Dimension" table="DIMENSIONES">
		<id name="id" access="field" type="java.lang.Long">
			<column name="ID_DIMENSION" />
			<generator class="native" />
		</id>
		<property name="nombre" access="field" type="java.lang.String">
			<column name="NOMBRE" />
		</property>
	</class>
</hibernate-mapping>