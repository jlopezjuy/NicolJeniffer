<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="e.dominio.Transaccion" table="TRANSACCIONES">
		<id name="id" access="field" type="java.lang.Long">
			<column name="ID_TRANSACCION" />
			<generator class="native" />
		</id>
		<property name="fechaTransaccion" access="field" type="java.util.Date">
			<column name="FECHA_TRANSACCION" />
		</property>
		<property name="idPersonaEmisor" access="field" type="java.lang.Long">
			<column name="ID_PERSONA_EMISOR" />
		</property>
		<property name="idPersonaReceptor" access="field" type="java.lang.Long">
			<column name="ID_PERSONA_RECEPTOR" />
		</property>
		<property name="idTipoTransaccion" access="field" type="java.lang.Long">
			<column name="ID_TIPO_TRANSACCION" />
		</property>
		<property name="idEstadoTransaccion" access="field" type="java.lang.Long">
			<column name="ID_ESTADO_TRANSACCION" />
		</property>
		<property name="interes" access="field" type="java.math.BigDecimal">
			<column name="INTERES" />
		</property>
		<property name="descuento" access="field" type="java.math.BigDecimal">
			<column name="DESCUENTO" />
		</property>
		<property name="ajuste" access="field" type="java.math.BigDecimal">
			<column name="AJUSTE" />
		</property>
	</class>
</hibernate-mapping>