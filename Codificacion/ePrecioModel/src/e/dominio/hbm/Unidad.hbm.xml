<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="e.dominio.Unidad" table="UNIDADES">
		<id name="id" access="field" type="java.lang.Long">
			<column name="ID_UNIDAD" />
			<generator class="native" />
		</id>
		<property name="nombre" access="field" type="java.lang.String">
			<column name="NOMBRE" />
		</property>
		<property name="idDimension" access="field" type="java.lang.Long">
			<column name="ID_DIMENSION" />
		</property>
		<property name="simbolo" access="field" type="java.lang.String">
			<column name="SIMBOLO" />
		</property>
		<property name="factorNumerador" access="field" type="java.lang.Integer">
			<column name="FACTOR_NUMERADOR" />
		</property>
		<property name="factorDenominador" access="field" type="java.lang.Integer">
			<column name="FACTOR_DENOMINADOR" />
		</property>
	</class>
</hibernate-mapping>