<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<locale-config>
			<default-locale>es_AR</default-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
	</application>
	<converter>
		<converter-id>dtoBaseNombreConverter</converter-id>
		<converter-class>e.converters.DTOBaseNombreConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>itemBean</managed-bean-name>
		<managed-bean-class>e.beans.ItemBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>productoListaBean</managed-bean-name>
		<managed-bean-class>e.beans.ProductoListaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>productoFormBean</managed-bean-name>
		<managed-bean-class>e.beans.ProductoFormBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>inicioBean</managed-bean-name>
		<managed-bean-class>e.beans.InicioBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>personaBean</managed-bean-name>
		<managed-bean-class>e.beans.PersonaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mensajeBean</managed-bean-name>
		<managed-bean-class>e.beans.MensajeBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>marcaListaBean</managed-bean-name>
		<managed-bean-class>e.beans.MarcaListaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>marcaFormBean</managed-bean-name>
		<managed-bean-class>e.beans.MarcaFormBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dimensionListaBean</managed-bean-name>
		<managed-bean-class>e.beans.DimensionListaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dimensionFormBean</managed-bean-name>
		<managed-bean-class>e.beans.DimensionFormBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>unidadFormBean</managed-bean-name>
		<managed-bean-class>e.beans.UnidadFormBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>precioListaBean</managed-bean-name>
		<managed-bean-class>e.beans.PrecioListaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>precioFormBean</managed-bean-name>
		<managed-bean-class>e.beans.PrecioFormBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Nuevos Beans -->
	
	<managed-bean>
		<managed-bean-name>clienteBean</managed-bean-name>
		<managed-bean-class>e.beans.ClienteBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- FIn nuevos beans -->

	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>ingresar</from-outcome>
			<to-view-id>/paginas/inicio.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>mensaje</from-outcome>
			<to-view-id>/paginas/mensaje.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>bienvenida</from-outcome>
			<to-view-id>/paginas/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>productoForm</from-outcome>
			<to-view-id>/paginas/productoForm.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>productoLista</from-outcome>
			<to-view-id>/paginas/productoLista.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>productoFormView</from-outcome>
			<to-view-id>/paginas/productoFormView.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>marcaForm</from-outcome>
			<to-view-id>/paginas/marcaForm.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>marcaLista</from-outcome>
			<to-view-id>/paginas/marcaLista.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>marcaFormView</from-outcome>
			<to-view-id>/paginas/marcaFormView.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>dimensionForm</from-outcome>
			<to-view-id>/paginas/dimensionForm.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>dimensionLista</from-outcome>
			<to-view-id>/paginas/dimensionLista.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>dimensionFormView</from-outcome>
			<to-view-id>/paginas/dimensionFormView.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>unidadForm</from-outcome>
			<to-view-id>/paginas/unidadForm.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>precioLista</from-outcome>
			<to-view-id>/paginas/precioLista.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>precioForm</from-outcome>
			<to-view-id>/paginas/precioForm.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>precioFormView</from-outcome>
			<to-view-id>/paginas/precioFormView.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>



</faces-config>
